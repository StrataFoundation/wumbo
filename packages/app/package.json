{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://app.wum.bo/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "defaults"
    ]
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "predeploy": "react-scripts build",
    "deploy": "echo 'wum.bo' > ./build/CNAME && gh-pages -d build",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "yarn format"
  },
  "dependencies": {
    "@chakra-ui/react": "1.7.4",
    "@metaplex-foundation/mpl-token-metadata": "^1.1.0",
    "@project-serum/anchor": "^0.20.1",
    "@project-serum/common": "^0.0.1-beta.3",
    "@solana/spl-token-registry": "^0.2.176",
    "@solana/wallet-adapter-base": "^0.9.3",
    "@solana/wallet-adapter-react": "^0.15.3",
    "@solana/wallet-adapter-wallets": "^0.15.4",
    "@solana/web3.js": "1.32.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@toruslabs/solana-embed": "0.0.9",
    "buffer": "^6.0.3",
    "framer-motion": "^4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-hook-form": "^7.11.1",
    "react-hot-toast": "2.1.0",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.3",
    "web-vitals": "^1.0.1",
    "wumbo-common": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "5.1.7",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.1.8",
    "webpack": "4.44.2"
  }
}
