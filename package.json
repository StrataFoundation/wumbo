{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "husky": "^7.0.0",
    "lerna": "^3.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "build": "lerna run build",
    "watch:common": "lerna exec yarn watch --scope wumbo-common",
    "watch:all": "lerna exec --parallel yarn run watch --scope wumbo-common --scope wumbo-extension",
    "start:site": "lerna exec yarn start --scope site",
    "prepare": "husky install"
  },
  "dependencies": {
    "react-icons": "^4.3.1"
  }
}
